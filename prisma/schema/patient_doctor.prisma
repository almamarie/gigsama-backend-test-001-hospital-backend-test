model PatientDoctor {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  patientId      String
  doctorId       String
  encryptedNotes String
  checkList      String

  plan Plan? @relation(fields: [planId], references: [planId])

  planId String? @unique
}
