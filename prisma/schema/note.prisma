model Note {
  id            String @id @default(uuid())
  patientId     String
  doctorId      String
  encryptedNote String

  checklist String
  plan      String

  patientDoctorId String
  patientDoctor   PatientDoctor @relation(fields: [patientDoctorId], references: [id])
  createdAt       DateTime      @default(now())
  updatedAt       DateTime      @updatedAt

  reminders Reminder[]
}
